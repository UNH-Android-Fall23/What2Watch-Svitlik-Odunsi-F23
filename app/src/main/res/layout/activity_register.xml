    <Button
        android:id="@+id/Registerbutton"
        android:layout_width="282dp"
        android:layout_height="61dp"
        android:text="Register"
        app:iconPadding="16dp"
        app:layout_constraintEnd_toEndOf="@+id/text_Home"
        app:layout_constraintStart_toStartOf="@+id/text_Home"
        app:layout_constraintTop_toBottomOf="@+id/editPassword"
        tools:ignore="HardcodedText" />
<!--
    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="281dp"
        android:layout_height="68dp"
        android:text="I agree to the terms of use and privacy"
        app:layout_constraintEnd_toEndOf="@+id/text_Home"
        app:layout_constraintStart_toStartOf="@+id/text_Home"
        app:layout_constraintTop_toBottomOf="@+id/Registerbutton"
        tools:ignore="HardcodedText" />
-->
    <EditText
        android:id="@+id/editPassword"
        android:layout_width="381dp"
        android:layout_height="58dp"
        android:layout_marginTop="8dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/text_Home"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toStartOf="@+id/text_Home"
        app:layout_constraintTop_toBottomOf="@id/editEmailAddress"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="74dp"
        android:layout_height="19dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-black"
        android:textColor="#070202"
        android:textSize="16sp"
        android:textStyle="bold"

        android:typeface="serif"
        app:layout_constraintEnd_toEndOf="@+id/text_Home"
        app:layout_constraintStart_toStartOf="@+id/text_Home"
        app:layout_constraintTop_toBottomOf="@+id/personalInfo"
        tools:text="Gender" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textColor="#020101"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Registerbutton"
        app:layout_constraintEnd_toEndOf="@+id/text_Home"
        app:layout_constraintHorizontal_bias="0.418"
        app:layout_constraintStart_toStartOf="@+id/text_Home"
        app:layout_constraintTop_toBottomOf="@+id/editTextDate"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="UnknownId"
        tools:text="Country" />

    <Spinner
        android:id="@+id/genderSpinner"
        android:layout_width="147dp"
        android:layout_height="33dp"
        android:layout_margin="16dp"

        android:layout_marginTop="16dp"
        android:text="@string/gender"
        app:layout_constraintEnd_toEndOf="@+id/text_Home"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/text_Home"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Spinner
        android:id="@+id/countrySpinner"
        android:layout_width="320dp"
        android:layout_height="35dp"
        android:layout_marginTop="4dp"
        android:text="Country"
        android:textColor="#0C0606"
        app:layout_constraintEnd_toEndOf="@+id/text_Home"
        app:layout_constraintHorizontal_bias="0.766"
        app:layout_constraintStart_toStartOf="@+id/text_Home"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />


</androidx.constraintlayout.widget.ConstraintLayout>